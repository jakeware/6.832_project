<?xml version="1.0" ?>

<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../../../pods/drake/doc/drakeURDF.xsd" name="ball">

  <link name="ball">
    <inertial>
      <origin xyz="0 0 -0.3" rpy="0 0 0" />
      <mass value="0.5"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -.3"/>
      <geometry>
        <sphere radius=".025"/>
      </geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0 0 -.3"/>
      <geometry>
        <sphere radius=".025"/>
      </geometry>
    </collision>
  </link>
  
</robot>